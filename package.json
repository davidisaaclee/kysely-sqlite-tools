    "@types/node": "^20.15.0",
    "@types/sql.js": "^1.4.9",
    "@vlcn.io/crsqlite-wasm": "^0.16.0",
    "better-sqlite3": "^11.1.2",
    "bumpp": "^9.5.1",
    "cz-git": "^1.9.4",
    "czg": "^1.9.4",
    "eslint": "^9.9.0",
    "kysely": "^0.27.4",
    "lint-staged": "^15.2.9",
    "node-sqlite3-wasm": "^0.8.19",
    "sql.js": "^1.11.0",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vite": "^5.4.1",
    "vitest": "^2.0.5"
