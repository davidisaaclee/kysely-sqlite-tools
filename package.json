{
  "name": "kysely-sqlite-tools",
  "type": "module",
  "version": "0.5.5",
  "private": true,
  "packageManager": "pnpm@8.14.0",
  "description": "",
  "author": {
    "name": "subframe7536",
    "email": "1667077010@qq.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subframe7536/kysely-sqlite-tools.git"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "pnpm -r -F \"./packages/*\" dev",
    "build": "pnpm -r -F \"./packages/*\" build",
    "release": "bumpp package.json packages/**/package.json",
    "serve": "cd playground && pnpm run dev",
    "publish": "pnpm -r -F \"./packages/*\" publish --no-git-checks",
    "test": "vitest run --root ./test --passWithNoTests",
    "test:dev": "vitest --root ./packages/sqlite-builder",
    "typecheck": "tsc --noEmit",
    "format": "eslint . --fix",
    "lint": "eslint .",
    "postinstall": "husky install",
    "commit": "czg"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@sqlite.org/sqlite-wasm": "3.43.2-prebuild1",
    "@subframe7536/eslint-config": "^0.5.9",
    "@types/better-sqlite3": "^7.6.8",
    "@types/node": "^20.10.7",
    "@types/sql.js": "^1.4.9",
    "@vlcn.io/crsqlite-wasm": "^0.16.0",
    "better-sqlite3": "^9.2.2",
    "bumpp": "^9.2.1",
    "cz-git": "^1.8.0",
    "czg": "^1.8.0",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "kysely": "^0.27.2",
    "lint-staged": "^15.2.0",
    "node-sqlite3-wasm": "^0.8.5",
    "sql.js": "^1.9.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.1.3"
  },
  "lint-staged": {
    "*": [
      "eslint . --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
