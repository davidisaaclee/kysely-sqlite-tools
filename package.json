{
  "name": "kysely-sqlite-tools",
  "type": "module",
  "version": "0.4.5",
  "private": true,
  "packageManager": "pnpm@8.10.0",
  "description": "",
  "author": {
    "name": "subframe7536",
    "email": "1667077010@qq.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subframe7536/kysely-sqlite-tools.git"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "pnpm -r -F \"./packages/*\" dev",
    "build": "pnpm -r -F \"./packages/*\" build",
    "release": "bumpp package.json packages/**/package.json",
    "serve": "cd playground && pnpm run dev",
    "publish": "pnpm -r -F \"./packages/*\" publish --no-git-checks",
    "test": "vitest run --root ./test --passWithNoTests",
    "test:dev": "vitest --root ./packages/sqlite-builder",
    "typecheck": "tsc --noEmit",
    "format": "eslint . --fix",
    "lint": "eslint .",
    "postinstall": "husky install",
    "commit": "czg"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@sqlite.org/sqlite-wasm": "3.43.2-prebuild1",
    "@subframe7536/eslint-config": "^0.3.7",
    "@subframe7536/wa-sqlite": "^0.9.6",
    "@types/better-sqlite3": "^7.6.6",
    "@types/node": "^18.18.7",
    "@types/sql.js": "^1.4.7",
    "@vlcn.io/crsqlite-wasm": "^0.15.2",
    "better-sqlite3": "^9.0.0",
    "bumpp": "^9.2.0",
    "cz-git": "^1.7.1",
    "czg": "^1.7.1",
    "eslint": "^8.52.0",
    "husky": "^8.0.3",
    "kysely": "^0.26.3",
    "lint-staged": "^14.0.1",
    "node-sqlite3-wasm": "^0.8.0",
    "sql.js": "^1.8.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "4.5.0",
    "vitest": "^0.34.6"
  },
  "lint-staged": {
    "*": [
      "eslint --cache --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
