      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./worker-classic": "./dist/worker.js",
    "./worker-module": "./dist/worker.mjs",
    "./wasm-sync": "./dist/wa-sqlite.wasm",
    "./wasm-async": "./dist/wa-sqlite-async.wasm"
