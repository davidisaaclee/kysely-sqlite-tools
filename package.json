{
  "name": "kysely-sqlite-tools",
  "type": "module",
  "version": "0.6.0",
  "private": true,
  "packageManager": "pnpm@8.15.5",
  "description": "",
  "author": {
    "name": "subframe7536",
    "email": "1667077010@qq.com"
  },
  "license": "MIT",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "pnpm -r -F \"./packages/*\" dev",
    "build": "pnpm -r -F \"./packages/*\" build",
    "release": "pnpm i && bumpp package.json packages/dialect*/package.json packages/plugin*/package.json",
    "serve": "cd playground && pnpm run dev",
    "publish": "pnpm -r -F \"./packages/*\" publish --no-git-checks",
    "test": "vitest run --root ./test --passWithNoTests",
    "test:dev": "vitest --root ./packages/sqlite-builder",
    "typecheck": "tsc --noEmit",
    "format": "eslint . --fix",
    "lint": "eslint .",
    "prepare": "husky",
    "commit": "czg"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@sqlite.org/sqlite-wasm": "^3.45.2-build1",
    "@subframe7536/eslint-config": "^0.5.13",
    "@subframe7536/type-utils": "^0.1.6",
    "@types/better-sqlite3": "^7.6.9",
    "@types/node": "^20.11.30",
    "@types/sql.js": "^1.4.9",
    "@vlcn.io/crsqlite-wasm": "^0.16.0",
    "better-sqlite3": "^9.4.3",
    "bumpp": "^9.4.0",
    "cz-git": "^1.9.0",
    "czg": "^1.9.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "kysely": "^0.27.3",
    "lint-staged": "^15.2.2",
    "node-sqlite3-wasm": "^0.8.13",
    "sql.js": "^1.10.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3",
    "vite": "^5.1.7",
    "vitest": "^1.4.0"
  },
  "lint-staged": {
    "*": [
      "eslint . --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
