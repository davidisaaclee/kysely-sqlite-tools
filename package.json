{
  "name": "kysely-sqlite-tools",
  "type": "module",
  "version": "0.6.3",
  "private": true,
  "packageManager": "pnpm@9.7.1",
  "description": "",
  "author": {
    "name": "subframe7536",
    "email": "1667077010@qq.com"
  },
  "license": "MIT",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "pnpm -r -F \"./packages/*\" dev",
    "build": "pnpm -r -F \"./packages/*\" build",
    "release": "pnpm i && bumpp package.json packages/dialect*/package.json packages/plugin*/package.json",
    "serve": "cd playground && pnpm run dev",
    "publish": "pnpm -r -F \"./packages/*\" publish --no-git-checks",
    "test": "vitest run --root ./test --passWithNoTests",
    "test:dev": "vitest --root ./packages/sqlite-builder",
    "typecheck": "tsc --noEmit",
    "format": "eslint . --fix",
    "lint": "eslint .",
    "prepare": "husky",
    "commit": "czg"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@sqlite.org/sqlite-wasm": "^3.46.0-build2",
    "@subframe7536/eslint-config": "^0.7.5",
    "@subframe7536/type-utils": "^0.1.6",
    "@types/better-sqlite3": "^7.6.11",
    "@types/node": "^20.15.0",
    "@types/sql.js": "^1.4.9",
    "@vlcn.io/crsqlite-wasm": "^0.16.0",
    "better-sqlite3": "^11.1.2",
    "bumpp": "^9.5.1",
    "cz-git": "^1.9.4",
    "czg": "^1.9.4",
    "eslint": "^9.9.0",
    "husky": "^9.1.4",
    "kysely": "^0.27.4",
    "lint-staged": "^15.2.9",
    "node-sqlite3-wasm": "^0.8.19",
    "sql.js": "^1.11.0",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vite": "^5.4.1",
    "vitest": "^2.0.5"
  },
  "lint-staged": {
    "*": [
      "eslint . --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
