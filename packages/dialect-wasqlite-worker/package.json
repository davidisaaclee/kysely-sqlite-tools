{
  "name": "kysely-wasqlite-worker",
  "version": "0.3.5",
  "description": "kysely dialect for wa-sqlite, running in worker",
  "author": {
    "name": "subframe7536",
    "email": "1667077010@qq.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/subframe7536/kysely-sqlite-tools#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subframe7536/kysely-sqlite-tools.git"
  },
  "bugs": "https://github.com/subframe7536/kysely-sqlite-tools/issues",
  "keywords": [
    "kysely",
    "dialect",
    "sql",
    "worker",
    "sqlite3"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./worker": {
      "import": "./dist/worker.mjs",
      "require": "./dist/worker.js"
    },
    "./*": "./dist/*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "wa-sqlite": "github:rhashimoto/wa-sqlite"
  },
  "dependencies": {
    "mitt": "^3.0.0"
  }
}